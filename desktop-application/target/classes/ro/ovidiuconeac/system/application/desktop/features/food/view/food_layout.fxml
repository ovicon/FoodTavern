<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.HBox?>
<StackPane
        xmlns:fx="http://javafx.com/fxml/1" fx:controller="ro.ovidiuconeac.system.application.desktop.features.food.view.FoodViewController"
        id="AnchorPane"
        prefHeight="200"
        prefWidth="320">
    <children>
        <StackPane id="fruits_pane">
            <HBox>
                <Button layoutX="126" layoutY="90" text="Get"  fx:id="getFruit" />
            </HBox>
        </StackPane>
        <StackPane id="cheeses_pane">
            <HBox>
                <Button layoutX="126" layoutY="90" text="Get"  fx:id="getCheese" />
            </HBox>
        </StackPane>
        <StackPane id="sweets_pane">
            <HBox>
                <Button layoutX="126" layoutY="90" text="Get"  fx:id="getSweet" />
            </HBox>
        </StackPane>

        <Button layoutX="126" layoutY="90" text="Click Me!"  fx:id="button" />
    </children>
</StackPane>
